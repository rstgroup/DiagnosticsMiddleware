{
    "name": "rstgroup/diagnostics-middleware",
    "description": "ZendDiagnostics PSR-7 middleware",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Witold Wasiczko",
            "email": "witold.wasiczko@rst.com.pl",
            "role": "Developer"
        }
    ],
    "keywords": [
        "middleware",
        "psr",
        "psr-7",
        "diagnostic"
    ],
    "require": {
        "php": ">=5.6",
        "zendframework/zenddiagnostics": "^1.1",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "zendframework/zend-diactoros": "^1.1",
        "php-middleware/double-pass-compatibility": "^1.0",
        "http-interop/http-middleware": "^0.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.26 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "RstGroup\\DiagnosticsMiddleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RstGroup\\DiagnosticsMiddleware\\Test\\": "test/"
        }
    }
}
